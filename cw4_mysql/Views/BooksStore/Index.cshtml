@model List<Book>

<h1>Strona główna</h1>
<table class="table table-striped">
<thead>
<th>Lp</th>
<th>Tytuł</th>
<th>Autor</th>
<th>Cena</th>
<th>Data wydania</th>
<th> --- </th>
</thead>
<tbody>
@{int Lp=0;}
@foreach(Book book in Model){
    Lp++;
   <tr>
   <td>@Lp</td>
   <td>@book.Title</td>
   <td>@book.Author</td>
   <td>@book.Price</td>
   <td>@book.RealizeDate.ToShortDateString()</td>
   <td> </td>
   </tr>
}
</tbody>
</table>